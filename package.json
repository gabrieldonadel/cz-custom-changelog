{
  "name": "cz-custom-changelog",
  "version": "0.0.7",
  "description": "Commitizen adapter following the conventional-changelog format.",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --require @babel/register *.test.js",
    "format": "prettier --write *.js",
    "prepublishOnly": "./node_modules/.bin/babel src --out-dir dist"
  },
  "homepage": "https://github.com/gabrieldonadel/cz-custom-changelog",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrieldonadel/cz-custom-changelog.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "author": "Gabriel Donadel Dall'Agnol",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "commitizen": "^4.0.3",
    "word-wrap": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cosmiconfig": "^5.2.1",
    "mocha": "^6.2.0",
    "mock-require": "^3.0.3",
    "prettier": "^2.0.5",
    "semantic-release": "^15.13.3",
    "semver": "^6.2.0"
  },
  "optionalDependencies": {
    "@commitlint/load": ">6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "dist/index.js"
    }
  }
}
